-- DROP TABLE `SCHEDULED`;

/**/ CREATE TABLE `SCHEDULED` (
  `ID` BIGINT AUTO_INCREMENT,
  `DESCRIPTION` VARCHAR(1024),
  `SCHEDULED` DATETIME,
  `VALUE` FLOAT NOT NULL,
  `TYPE` VARCHAR(15),
  `BOOKED` TINYINT(1),
  `NOTES` VARCHAR(1024),
  `ADDRESS` VARCHAR(15) NOT NULL,
  `TENANT` VARCHAR(36) NOT NULL,
  `CREATED` DATETIME,
  CONSTRAINT `PK_Scheduled` PRIMARY KEY (`ID`)
); /**/

/*--*/ CREATE TRIGGER `SCHEDULED_CREATED_DEFAULT` BEFORE INSERT ON `SCHEDULED`
/*--*/   FOR EACH ROW SET NEW.`CREATED` = CURRENT_TIMESTAMP;


